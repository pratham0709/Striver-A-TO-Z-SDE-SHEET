class Solution {
public:
    bool isAnagram(string s, string t) {
       unordered_set<char>set;
        
        for(int i =0; i<s.length(); i++){
            if(set[i]==0){
                set.insert(s[i]);
            }
            else{
                set.insert(s[i]);
            }
        }
        
        unordered_set<char>map;
        
        for(int i = 0; i<t.length(); i++){
            if(map[i]==0){
                map.insert(t[i]);
            }
            else{
                map.insert(t[i]);
            }
        }
        
        if(set == map){
            return true;
        }
        else{
            return false;
        }
}
};
